    "/**\n",
    " * Implement a program that determines if a is divisible by b\n",
    " */\n",
    "\n",
    "int a = 119;\n",
    "int b = 17;\n",
    "\n",
    "if (a % b == 0) {\n",
    "    display(\"b divides a \" + (a / b) + \" times\");\n",
    "} else {\n",
    "    display(\"b doesn't divide a\");\n",
    "}"
